my_file <- list.files()
my_data <- read.csv2(my_file)

my_data$Date <- as.character(my_data$Date)
my_data$Date <- as.Date(my_data$Date,"%y/%m/%d")
startdate <- as.Date("2017-02-01")
enddate <- as.Date("2017-02-03")
newdata <- my_data[which(my_data$Date >= startdate &
                             my_data$Date <= enddate),]
# it is so strange when I process line 4, the date 20/12/2006 change into 2016-12-20 !!! Ten years!
